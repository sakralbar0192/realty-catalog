{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "analyze": "ANALYZE=true bun run build",
    "analyze:serve": "bun run analyze && npx serve dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "stylelint": "stylelint '**/*.{vue,scss,css}'",
    "stylelint:fix": "stylelint '**/*.{vue,scss,css}' --fix",
    "stylelint:check": "stylelint '**/*.{vue,scss,css}' --max-warnings 0",
    "html-validate": "html-validate '**/*.{html,vue}'",
    "html-validate:check": "html-validate 'app.vue' 'pages/**/*.vue' 'components/**/*.vue'"
  },
  "dependencies": {
    "@nuxt/fonts": "0.11.4",
    "@nuxt/image": "1.11.0",
    "@nuxtjs/i18n": "^10.1.0",
    "@pinia/nuxt": "^0.11.2",
    "nuxt": "^3.19.2",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@mswjs/data": "^0.16.2",
    "@nuxt/eslint-config": "^1.9.0",
    "@types/node": "^24.5.2",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.4.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "msw": "^2.11.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.93.0",
    "stylelint": "^16.24.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "vite-bundle-analyzer": "^1.2.3",
    "web-vitals": "^5.1.0"
  }
}
